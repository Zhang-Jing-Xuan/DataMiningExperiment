                <mxCell id="55" value="&lt;span style=&quot;font-size: 20px&quot;&gt;速度快&lt;br&gt;&lt;/span&gt;" style="text;html=1;align=center;verticalAlign=middle;resizable=0;points=[];autosize=1;" parent="1" vertex="1">
